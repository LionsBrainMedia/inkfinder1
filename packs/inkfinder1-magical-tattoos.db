{"name":"Diagnostic Display","type":"equipment","system":{"description":{"value":"<p class=\"PF2StatBody\">This rectangle tattooed onto the wearer&rsquo;s chest displays numbers and descriptions about their health and any medical abnormalities or threats. This grants a +2 item bonus on checks to any creature attempting to identify diseases the wearer has.</p>"},"source":{"value":"Inkfinder"},"traits":{"value":["invested","magical","tattoo"],"rarity":"common","custom":"","otherTags":[]},"rules":[],"slug":null,"schema":{"version":0.794,"lastMigration":null},"quantity":1,"baseItem":null,"hp":{"brokenThreshold":0,"value":0,"max":0},"hardness":0,"weight":{"value":"-"},"equippedBulk":{"value":""},"price":{"value":{"gp":50}},"equipped":{"carryType":"worn","invested":null},"stackGroup":null,"negateBulk":{"value":"0"},"containerId":null,"preciousMaterial":{"value":null},"preciousMaterialGrade":{"value":null},"size":"med","identification":{"status":"identified","unidentified":{"name":"Unusual Object","img":"systems/pf2e/icons/unidentified_item_icons/adventuring_gear.webp","data":{"description":{"value":""}}},"misidentified":{}},"usage":{"value":"tattooed-on-the-body"},"level":{"value":3}},"img":"modules/inkfinder1/art/inkfinder1%20cover.png","effects":[],"ownership":{"default":0,"AZ0bdgVQUIyrXfSF":3},"flags":{"core":{"sourceId":"Item.EShmT13BWQrU7xdS"}},"_stats":{"systemId":"pf2e","systemVersion":"4.2.6","coreVersion":"10.288","createdTime":1665960228205,"modifiedTime":1666034761920,"lastModifiedBy":"AZ0bdgVQUIyrXfSF"},"folder":null,"sort":0,"_id":"k0I10F5hiuIILSAE"}
